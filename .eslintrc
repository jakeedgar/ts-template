{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
      "prettier"
  ],
  "rules": {
    "no-console": 1,
    "semi": 1,
    "indent": ["warn", 2],
    "no-debugger": "warn",
    "prettier/prettier": 2
  }
}